    {
      "cell_type": "code",
      "source": [
        "X = np.array([[0,0],\n",
        "             [0,1],\n",
        "             [1,0],\n",
        "             [1,1]])\n",
        "\n",
        "y = np.array([[1],\n",
        "      [0],\n",
        "      [0],\n",
        "      [1]])\n"
      ],
      "metadata": {
        "id": "KR4ySYovcs9b"
      },
      "execution_count": 70,
      "outputs": []
    },
    {
      "cell_type": "code",
